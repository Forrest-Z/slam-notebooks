   "source": [
    "# g2o\n",
    "\n",
    "Install dependencies\n",
    "\n",
    "```bash\n",
    "sudo apt-get install cmake libeigen3-dev libsuoteparse-dev qtdeclarative5-dev qt5-qmake libqglviewer-dev\n",
    "```\n",
    "\n",
    "Clone\n",
    "\n",
    "```bash\n",
    "git clone https://github.com/RainerKuemmerle/g2o.git\n",
    "cd g2o\n",
    "```\n",
    "\n",
    "Compile\n",
    "\n",
    "\n",
    "```bash\n",
    "mkdir build\n",
    "cd build\n",
    "cmake ../\n",
    "make\n",
    "```"
   ]
